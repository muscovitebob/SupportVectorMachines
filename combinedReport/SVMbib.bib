@article{dewolfTranscriptionalCharacterizationCompounds2016,
  title = {Transcriptional {{Characterization}} of {{Compounds}}: {{Lessons Learned}} from the {{Public LINCS Data}}},
  volume = {14},
  issn = {1540-658X, 1557-8127},
  shorttitle = {Transcriptional {{Characterization}} of {{Compounds}}},
  abstract = {The NIH-funded LINCS program has been initiated to generate a library of integrated, network-based, cellular signatures (LINCS). A novel high-throughput gene-expression profiling assay known as L1000 was the main technology used to generate more than a million transcriptional profiles. The profiles are based on the treatment of 14 cell lines with one of many perturbation agents of interest at a single concentration for 6 and 24 hours duration. In this study, we focus on the chemical compound treatments within the LINCS data set. The experimental variables available include number of replicates, cell lines, and time points. Our study reveals that compound characterization based on three cell lines at two time points results in more genes being affected than six cell lines at a single time point. Based on the available LINCS data, we conclude that the most optimal experimental design to characterize a large set of compounds is to test them in duplicate in three different cell lines. Our conclusions are constrained by the fact that the compounds were profiled at a single, relative high concentration, and the longer time point is likely to result in phenotypic rather than mechanistic effects being recorded.},
  language = {en},
  number = {4},
  journal = {ASSAY and Drug Development Technologies},
  doi = {10.1089/adt.2016.715},
  author = {De Wolf, Hans and De Bondt, An and Turner, Heather and G{\"o}hlmann, Hinrich W.H},
  month = may,
  year = {2016},
  pages = {252-260},
  file = {/Users/bob/Zotero/storage/N98V2GY3/De Wolf et al. - 2016 - Transcriptional Characterization of Compounds Les.pdf}
}

@article{dewolfHighThroughputGeneExpression2018,
  title = {High-{{Throughput Gene Expression Profiles}} to {{Define Drug Similarity}} and {{Predict Compound Activity}}},
  volume = {16},
  issn = {1540-658X, 1557-8127},
  abstract = {By adding biological information, beyond the chemical properties and desired effect of a compound, uncharted compound areas and connections can be explored. In this study, we add transcriptional information for 31K compounds of Janssen's primary screening deck, using the HT L1000 platform and assess (a) the transcriptional connection score for generating compound similarities, (b) machine learning algorithms for generating target activity predictions, and (c) the scaffold hopping potential of the resulting hits. We demonstrate that the transcriptional connection score is best computed from the significant genes only and should be interpreted within its confidence interval for which we provide the stats. These guidelines help to reduce noise, increase reproducibility, and enable the separation of specific and promiscuous compounds. The added value of machine learning is demonstrated for the NR3C1 and HSP90 targets. Support Vector Machine models yielded balanced accuracy values \textdaggerdbl{}80\% when the expression values from DDIT4 \& SERPINE1 and TMEM97 \& SPR were used to predict the NR3C1 and HSP90 activity, respectively. Combining both models resulted in 22 new and confirmed HSP90-independent NR3C1 inhibitors, providing two scaffolds (i.e., pyrimidine and pyrazolo-pyrimidine), which could potentially be of interest in the treatment of depression (i.e., inhibiting the glucocorticoid receptor (i.e., NR3C1), while leaving its chaperone, HSP90, unaffected). As such, the initial hit rate increased by a factor 300, as less, but more specific chemistry could be screened, based on the upfront computed activity predictions.},
  language = {en},
  number = {3},
  journal = {ASSAY and Drug Development Technologies},
  doi = {10.1089/adt.2018.845},
  author = {De Wolf, Hans and Cougnaud, Laure and Van Hoorde, Kirsten and De Bondt, An and Wegner, Joerg K. and Ceulemans, Hugo and G{\"o}hlmann, Hinrich},
  month = apr,
  year = {2018},
  pages = {162-176},
  file = {/Users/bob/Zotero/storage/YE9N5VQQ/De Wolf et al. - 2018 - High-Throughput Gene Expression Profiles to Define.pdf}
}

