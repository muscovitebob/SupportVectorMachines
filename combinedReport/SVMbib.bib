@article{wangSystematicDrugSafety2015,
  title = {Systematic Drug Safety Evaluation Based on Public Genomic Expression ({{Connectivity Map}}) Data: {{Myocardial}} and Infectious Adverse Reactions as Application Cases},
  volume = {457},
